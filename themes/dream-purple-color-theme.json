  "tokenColors": [   
 {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#8b7a9b"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.control",
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.other.using",
        "keyword.other.directive.using"
      ],
      "settings": {
        "foreground": "#ff6b35"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "string.quoted",
        "string.template",
        "string.interpolated"
      ],
      "settings": {
        "foreground": "#64ffda"
      }
    },
    {
      "name": "String Escape",
      "scope": [
        "constant.character.escape",
        "string.regexp"
      ],
      "settings": {
        "foreground": "#ff8c42"
      }
    },
    {
      "name": "Number",
      "scope": [
        "constant.numeric",
        "constant.language.boolean",
        "constant.language.null",
        "constant.language.undefined"
      ],
      "settings": {
        "foreground": "#ff8c42"
      }
    },
    {
      "name": "Function",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#82b1ff"
      }
    },
    {
      "name": "Function Parameters",
      "scope": [
        "variable.parameter",
        "meta.parameters"
      ],
      "settings": {
        "foreground": "#f4f1f4",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Class",
      "scope": [
        "entity.name.class",
        "entity.name.type",
        "support.type",
        "support.class",
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#c792ea"
      }
    },
    {
      "name": "Variable",
      "scope": [
        "variable",
        "string constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#f4f1f4"
      }
    },
    {
      "name": "Object Property",
      "scope": [
        "variable.other.property",
        "variable.other.object.property",
        "meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#82b1ff"
      }
    },
    {
      "name": "Operator",
      "scope": [
        "keyword.operator",
        "keyword.operator.arithmetic",
        "keyword.operator.comparison",
        "keyword.operator.logical",
        "keyword.operator.assignment"
      ],
      "settings": {
        "foreground": "#ff6b35"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "punctuation.definition",
        "punctuation.separator",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#8b7a9b"
      }
    },
    {
      "name": "Brackets",
      "scope": [
        "punctuation.definition.block",
        "punctuation.definition.array",
        "punctuation.definition.parameters",
        "punctuation.definition.group"
      ],
      "settings": {
        "foreground": "#ff8c42"
      }
    },
    {
      "name": "JS/TS Import/Export",
      "scope": [
        "keyword.control.import",
        "keyword.control.export",
        "keyword.control.from",
        "keyword.control.as"
      ],
      "settings": {
        "foreground": "#ff6b35"
      }
    },
    {
      "name": "JS/TS This",
      "scope": [
        "variable.language.this",
        "variable.language.super"
      ],
      "settings": {
        "foreground": "#c792ea",
        "fontStyle": "italic"
      }
    },
    {
      "name": "JS/TS Template Literals",
      "scope": [
        "punctuation.definition.template-expression",
        "string.template meta.embedded"
      ],
      "settings": {
        "foreground": "#ff8c42"
      }
    },
    {
      "name": "TypeScript Types",
