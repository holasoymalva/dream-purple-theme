{
  "name": "Dream Purple",
  "type": "dark",
  "colors": {
    // Base editor colors
    "editor.background": "#1a0d26",
    "editor.foreground": "#f4f1f4",
    "editor.selectionBackground": "#2d1b3d80",
    "editor.selectionHighlightBackground": "#2d1b3d60",
    "editor.lineHighlightBackground": "#2d1b3d40",
    "editorCursor.foreground": "#ff6b35",
    "editorWhitespace.foreground": "#8b7a9b40",
    
    // Activity Bar colors
    "activityBar.background": "#1a0d26",
    "activityBar.foreground": "#ff6b35",
    "activityBar.inactiveForeground": "#8b7a9b",
    "activityBar.border": "#2d1b3d",
    "activityBarBadge.background": "#ff6b35",
    "activityBarBadge.foreground": "#1a0d26",
    
    // Sidebar colors
    "sideBar.background": "#2d1b3d",
    "sideBar.foreground": "#f4f1f4",
    "sideBar.border": "#4a2c5a",
    "sideBarTitle.foreground": "#ff6b35",
    "sideBarSectionHeader.background": "#1a0d26",
    "sideBarSectionHeader.foreground": "#ff8c42",
    
    // Status Bar colors
    "statusBar.background": "#1a0d26",
    "statusBar.foreground": "#f4f1f4",
    "statusBar.border": "#2d1b3d",
    "statusBarItem.hoverBackground": "#2d1b3d",
    "statusBarItem.prominentBackground": "#ff6b35",
    "statusBarItem.prominentForeground": "#1a0d26",
    
    // Tab colors and active/inactive states
    "tab.activeBackground": "#1a0d26",
    "tab.activeForeground": "#f4f1f4",
    "tab.activeBorder": "#ff6b35",
    "tab.activeBorderTop": "#ff6b35",
    "tab.inactiveBackground": "#2d1b3d",
    "tab.inactiveForeground": "#8b7a9b",
    "tab.border": "#4a2c5a",
    "tab.hoverBackground": "#4a2c5a",
    "tab.hoverForeground": "#f4f1f4",
    "tab.hoverBorder": "#ff8c42",
    "tab.unfocusedActiveBackground": "#2d1b3d",
    "tab.unfocusedActiveForeground": "#8b7a9b",
    "tab.unfocusedActiveBorder": "#4a2c5a",
    "tab.unfocusedInactiveBackground": "#2d1b3d",
    "tab.unfocusedInactiveForeground": "#8b7a9b60",
    "tabsContainer.border": "#4a2c5a",
    
    // Terminal panel colors and integrated terminal styling
    "terminal.background": "#1a0d26",
    "terminal.foreground": "#f4f1f4",
    "terminal.ansiBlack": "#1a0d26",
    "terminal.ansiRed": "#ff6b6b",
    "terminal.ansiGreen": "#64ffda",
    "terminal.ansiYellow": "#ff8c42",
    "terminal.ansiBlue": "#82b1ff",
    "terminal.ansiMagenta": "#c792ea",
    "terminal.ansiCyan": "#64ffda",
    "terminal.ansiWhite": "#f4f1f4",
    "terminal.ansiBrightBlack": "#8b7a9b",
    "terminal.ansiBrightRed": "#ff8a80",
    "terminal.ansiBrightGreen": "#69f0ae",
    "terminal.ansiBrightYellow": "#ffd54f",
    "terminal.ansiBrightBlue": "#82b1ff",
    "terminal.ansiBrightMagenta": "#e1bee7",
    "terminal.ansiBrightCyan": "#84ffff",
    "terminal.ansiBrightWhite": "#ffffff",
    "terminal.selectionBackground": "#ff6b3540",
    "terminal.border": "#4a2c5a",
    "terminalCursor.background": "#1a0d26",
    "terminalCursor.foreground": "#ff6b35",
    
    // Panel colors
    "panel.background": "#1a0d26",
    "panel.border": "#4a2c5a",
    "panelTitle.activeBorder": "#ff6b35",
    "panelTitle.activeForeground": "#f4f1f4",
    "panelTitle.inactiveForeground": "#8b7a9b",
    "panelInput.border": "#4a2c5a",
    
    // Input field colors and dropdown styling
    "input.background": "#2d1b3d",
    "input.foreground": "#f4f1f4",
    "input.border": "#4a2c5a",
    "input.placeholderForeground": "#8b7a9b",
    "inputOption.activeBorder": "#ff6b35",
    "inputOption.activeBackground": "#ff6b3520",
    "inputOption.activeForeground": "#f4f1f4",
    "inputValidation.errorBackground": "#ff6b6b20",
    "inputValidation.errorBorder": "#ff6b6b",
    "inputValidation.errorForeground": "#ff6b6b",
    "inputValidation.infoBackground": "#82b1ff20",
    "inputValidation.infoBorder": "#82b1ff",
    "inputValidation.infoForeground": "#82b1ff",
    "inputValidation.warningBackground": "#ff8c4220",
    "inputValidation.warningBorder": "#ff8c42",
    "inputValidation.warningForeground": "#ff8c42",
    
    // Dropdown styling
    "dropdown.background": "#2d1b3d",
    "dropdown.foreground": "#f4f1f4",
    "dropdown.border": "#4a2c5a",
    "dropdown.listBackground": "#2d1b3d",
    
    // Button styling
    "button.background": "#ff6b35",
    "button.foreground": "#1a0d26",
    "button.hoverBackground": "#ff8c42",
    "button.border": "#ff6b35",
    "button.secondaryBackground": "#4a2c5a",
    "button.secondaryForeground": "#f4f1f4",
    "button.secondaryHoverBackground": "#6b4c7a",
    
    // Notification and tooltip colors
    "notifications.background": "#2d1b3d",
    "notifications.foreground": "#f4f1f4",
    "notifications.border": "#4a2c5a",
    "notificationCenter.border": "#4a2c5a",
    "notificationCenterHeader.background": "#1a0d26",
    "notificationCenterHeader.foreground": "#f4f1f4",
    "notificationToast.border": "#4a2c5a",
    "notificationsErrorIcon.foreground": "#ff6b6b",
    "notificationsWarningIcon.foreground": "#ff8c42",
    "notificationsInfoIcon.foreground": "#82b1ff",
    
    // Tooltip colors
    "editorHoverWidget.background": "#2d1b3d",
    "editorHoverWidget.foreground": "#f4f1f4",
    "editorHoverWidget.border": "#4a2c5a",
    "editorHoverWidget.statusBarBackground": "#1a0d26",
    
    // Additional interactive elements
    "badge.background": "#ff6b35",
    "badge.foreground": "#1a0d26",
    "progressBar.background": "#ff6b35",
    
    // List and tree styling for better UI consistency
    "list.activeSelectionBackground": "#ff6b3530",
    "list.activeSelectionForeground": "#f4f1f4",
    "list.inactiveSelectionBackground": "#4a2c5a30",
    "list.inactiveSelectionForeground": "#f4f1f4",
    "list.hoverBackground": "#4a2c5a20",
    "list.hoverForeground": "#f4f1f4",
    "list.focusBackground": "#ff6b3540",
    "list.focusForeground": "#f4f1f4",
    "list.highlightForeground": "#ff6b35",
    "listFilterWidget.background": "#2d1b3d",
    "listFilterWidget.outline": "#ff6b35",
    "listFilterWidget.noMatchesOutline": "#ff6b6b",
    
    // Tree styling
    "tree.indentGuidesStroke": "#4a2c5a",
    "tree.tableColumnsBorder": "#4a2c5a"
  },
  "tokenColors": [
    // Comments
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#8b7a9b"
      }
    },
    
    // Keywords
    {
      "name": "Keyword",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.control",
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.other.using",
        "keyword.other.directive.using"
      ],
      "settings": {
        "foreground": "#ff6b35"
      }
    },
    
    // Strings
    {
      "name": "String",
      "scope": [
        "string",
        "string.quoted",
        "string.template",
        "string.interpolated"
      ],
      "settings": {
        "foreground": "#64ffda"
      }
    },
    
    // String escape characters
    {
      "name": "String Escape",
      "scope": [
        "constant.character.escape",
        "string.regexp"
      ],
      "settings": {
        "foreground": "#ff8c42"
      }
    },
    
    // Numbers
    {
      "name": "Number",
      "scope": [
        "constant.numeric",
        "constant.language.boolean",
        "constant.language.null",
        "constant.language.undefined"
      ],
      "settings": {
        "foreground": "#ff8c42"
      }
    },
    
    // Functions
    {
      "name": "Function",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#82b1ff"
      }
    },
    
    // Function parameters
    {
      "name": "Function Parameters",
      "scope": [
        "variable.parameter",
        "meta.parameters"
      ],
      "settings": {
        "foreground": "#f4f1f4",
        "fontStyle": "italic"
      }
    },
    
    // Classes and Types
    {
      "name": "Class",
      "scope": [
        "entity.name.class",
        "entity.name.type",
        "support.type",
        "support.class",
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#c792ea"
      }
    },
    
    // Variables
    {
      "name": "Variable",
      "scope": [
        "variable",
        "string constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#f4f1f4"
      }
    },
    
    // Object properties
    {
      "name": "Object Property",
      "scope": [
        "variable.other.property",
        "variable.other.object.property",
        "meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#82b1ff"
      }
    },
    
    // Operators
    {
      "name": "Operator",
      "scope": [
        "keyword.operator",
        "keyword.operator.arithmetic",
        "keyword.operator.comparison",
        "keyword.operator.logical",
        "keyword.operator.assignment"
      ],
      "settings": {
        "foreground": "#ff6b35"
      }
    },
    
    // Punctuation
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "punctuation.definition",
        "punctuation.separator",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#8b7a9b"
      }
    },
    
    // Brackets and Braces
    {
      "name": "Brackets",
      "scope": [
        "punctuation.definition.block",
        "punctuation.definition.array",
        "punctuation.definition.parameters",
        "punctuation.definition.group"
      ],
      "settings": {
        "foreground": "#ff8c42"
      }
    },
    
    // JavaScript/TypeScript specific
    {
      "name": "JS/TS Import/Export",
      "scope": [
        "keyword.control.import",
        "keyword.control.export",
        "keyword.control.from",
        "keyword.control.as"
      ],
      "settings": {
        "foreground": "#ff6b35"
      }
    },
    
    {
      "name": "JS/TS This",
      "scope": [
        "variable.language.this",
        "variable.language.super"
      ],
      "settings": {
        "foreground": "#c792ea",
        "fontStyle": "italic"
      }
    },
    
    {
      "name": "JS/TS Template Literals",
      "scope": [
        "punctuation.definition.template-expression",
        "string.template meta.embedded"
      ],
      "settings": {
        "foreground": "#ff8c42"
      }
    },
    
    // TypeScript specific
    {
      "name": "TypeScript Types",
      "scope": [
        "entity.name.type.ts",
        "entity.name.type.tsx",
        "support.type.primitive",
        "keyword.type"
      ],
      "settings": {
        "foreground": "#c792ea"
      }
    },
    
    {
      "name": "TypeScript Interface",
      "scope": [
        "entity.name.type.interface",
        "keyword.other.interface"
      ],
      "settings": {
        "foreground": "#82b1ff"
      }
    },
    
    // HTML specific
    {
      "name": "HTML Tag",
      "scope": [
        "entity.name.tag",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#ff6b35"
      }
    },
    
    {
      "name": "HTML Attribute",
      "scope": [
        "entity.other.attribute-name",
        "meta.attribute"
      ],
      "settings": {
        "foreground": "#82b1ff"
      }
    },
    
    {
      "name": "HTML Attribute Value",
      "scope": [
        "string.quoted.double.html",
        "string.quoted.single.html"
      ],
      "settings": {
        "foreground": "#64ffda"
      }
    },
    
    {
      "name": "HTML Doctype",
      "scope": [
        "meta.tag.sgml.doctype",
        "string.quoted.docinfo.doctype"
      ],
      "settings": {
        "foreground": "#8b7a9b"
      }
    },
    
    // CSS specific
    {
      "name": "CSS Property",
      "scope": [
        "support.type.property-name.css",
        "meta.property-name.css"
      ],
      "settings": {
        "foreground": "#82b1ff"
      }
    },
    
    {
      "name": "CSS Property Value",
      "scope": [
        "support.constant.property-value.css",
        "meta.property-value.css"
      ],
      "settings": {
        "foreground": "#64ffda"
      }
    },
    
    {
      "name": "CSS Selector",
      "scope": [
        "entity.name.tag.css",
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.id.css"
      ],
      "settings": {
        "foreground": "#ff6b35"
      }
    },
    
    {
      "name": "CSS Units",
      "scope": [
        "keyword.other.unit.css"
      ],
      "settings": {
        "foreground": "#ff8c42"
      }
    },
    
    {
      "name": "CSS Pseudo Classes",
      "scope": [
        "entity.other.attribute-name.pseudo-class.css",
        "entity.other.attribute-name.pseudo-element.css"
      ],
      "settings": {
        "foreground": "#c792ea"
      }
    },
    
    // JSON specific
    {
      "name": "JSON Key",
      "scope": [
        "support.type.property-name.json",
        "meta.structure.dictionary.key.json"
      ],
      "settings": {
        "foreground": "#82b1ff"
      }
    },
    
    {
      "name": "JSON String",
      "scope": [
        "string.quoted.double.json"
      ],
      "settings": {
        "foreground": "#64ffda"
      }
    },
    
    {
      "name": "JSON Number",
      "scope": [
        "constant.numeric.json"
      ],
      "settings": {
        "foreground": "#ff8c42"
      }
    },
    
    {
      "name": "JSON Boolean/Null",
      "scope": [
        "constant.language.json"
      ],
      "settings": {
        "foreground": "#c792ea"
      }
    },
    
    // Markdown specific
    {
      "name": "Markdown Heading",
      "scope": [
        "markup.heading",
        "entity.name.section.markdown"
      ],
      "settings": {
        "foreground": "#ff6b35",
        "fontStyle": "bold"
      }
    },
    
    {
      "name": "Markdown Bold",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "foreground": "#ff8c42",
        "fontStyle": "bold"
      }
    },
    
    {
      "name": "Markdown Italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "foreground": "#c792ea",
        "fontStyle": "italic"
      }
    },
    
    {
      "name": "Markdown Code",
      "scope": [
        "markup.inline.raw",
        "markup.fenced_code"
      ],
      "settings": {
        "foreground": "#64ffda"
      }
    },
    
    {
      "name": "Markdown Link",
      "scope": [
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#82b1ff"
      }
    }
  ]
}